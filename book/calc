// Calc.cs
using System;
namespace CalculatorExample
{
    // class System 
    class Program
    {
        static void Main(string[] args)   
        {
            Calc c = new Calc();
            int ans =c.Add(10, 84);
            Console.WriteLine("10 + 84 is {0}.", ans);
            Console.ReadLine();
        }

    }
  // Class Calculator C#
  class Calc{
    public  int Add(int addend1, int addend2)
    {
      return  addend1 + addend2;
    }
  }
}